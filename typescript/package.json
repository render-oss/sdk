    "@types/eventsource": "^3.0.0",
    "@types/node": "^20.0.0",
    "openapi-typescript": "^7.10.1",
    "typescript": "^5.9.3",
    "vitest": "^1.0.0"
  },
  "dependencies": {
    "eventsource": "^4.0.0",
    "openapi-fetch": "^0.15.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "exports": {
    "./workflows": {
      "import": "./dist/workflows/index.js",
